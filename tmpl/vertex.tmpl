{{- if .Canceled -}}
{{- Foreground "11" "CANCELED" -}}
{{- else if .Error -}}
{{- Foreground "1" "ERROR" -}}
{{- else if .Cached -}}
{{- Foreground "4" "CACHED" -}}
{{- else if and .Started (not .Completed) -}}
{{- Foreground "3" (printf "[%s]" (.Duration | duration)) -}}
{{- else -}}
{{- Foreground "8" (printf "[%s]" (.Duration | duration)) -}}
{{- end -}}
{{- " " -}}
{{- .Name -}}
{{- "" }}
